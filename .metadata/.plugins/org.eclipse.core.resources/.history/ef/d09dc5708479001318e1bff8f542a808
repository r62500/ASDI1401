var obj6 = new Object();
obj6.name = "Object 6";
obj6.length = 2;
obj6.width = 9;
obj6.area = function() {
	return obj6.length * obj6.width;
};
obj6.announce = function() {
	return "I am " + obj6.name + ". I have a height of " + obj6.length + ". I have a width of " + obj6.width + ". This means that my area is " + obj6.area + ".";
};

